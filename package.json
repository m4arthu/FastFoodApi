{
  "name": "fastfoodapi",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js",
    "build": "npx prisma migrate deploy",
    "dev": "npx dotenv -e .env.development  nodemon ./src/server.js",
    "dev:migrate": "npx dotenv -e .env.development prisma migrate dev",
    "test": "npx dotenv -e .env.test jest",
    "test:coverage": "npx dotenv -e .env.test jest -- --coverage",
    "test:migrate": "npx dotenv -e .env.test prisma migrate dev",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.3.1",
    "@prisma/client": "^5.3.1",
    "babel-jest": "^29.7.0",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.3.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "~1.6.3",
    "jest": "^29.7.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^8.0.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.1",
    "supertest": "^6.3.3"
  }
}
